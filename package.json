{
  "name": "angular2-rest-client",
  "version": "3.0.2",
  "description": "Angular2 REST Client",
  "main": "index.js",
  "scripts": {
    "preversion": "npm test",
    "test": "karma start",
    "tsc": "tsc",
    "ngc": "./node_modules/.bin/ngc",
    "rollup": "rollup",
    "copy:dist": "cp src/package.json dist/ && cp README.md dist/",
    "copy:version": "cd dist && npm version $(cat ../package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g')",
    "rollup:dist": "npm run rollup -- -c rollup.config.js",
    "build:dist": "npm run ngc -- -p tsconfig.build.json && npm run rollup:dist && npm run copy:dist && npm run copy:version",
    "prebuild:dist": "rm -rf dist/*",
    "postbuild:dist": "find dist/ -type f -name '*.d.ts' -print0 | xargs -0 sed -i '' -e 's/\\/\\/\\/ <reference types=\"core-js\" \\/>//g'"
  },
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gizm0bill/angular2-rest-client.git"
  },
  "keywords": [
    "angular2",
    "http",
    "rest",
    "client"
  ],
  "author": "Mihai Billy Bălăceanu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gizm0bill/angular2-rest-client/issues"
  },
  "homepage": "https://github.com/gizm0bill/angular2-rest-client",
  "dependencies": {
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "core-js": "^2.5.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.8",
    "zone.js": "^0.8.25"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/platform-server": "^5.2.9",
    "@types/core-js": "^0.9.46",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.6.2",
    "@types/source-map": "^0.5.7",
    "@types/webpack": "^4.1.3",
    "awesome-typescript-loader": "^4.0.1",
    "codelyzer": "^4.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "~3.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "phantomjs": "^2.1.7",
    "rollup": "^0.57.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0"
  }
}
