{
  "name": "angular2-rest-client",
  "version": "3.0.5",
  "description": "Angular2 REST Client",
  "main": "index.js",
  "scripts": {
    "preversion": "npm test",
    "test": "karma start",
    "tsc": "tsc",
    "ngc": "./node_modules/.bin/ngc",
    "rollup": "rollup",
    "copy:dist": "cp src/package.json dist/ && cp README.md dist/",
    "copy:version": "cd dist && npm version $(cat ../package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g')",
    "rollup:dist": "npm run rollup -- -c rollup.config.js",
    "build:dist": "npm run ngc -- -p tsconfig.build.json && npm run rollup:dist && npm run copy:dist && npm run copy:version",
    "prebuild:dist": "rm -rf dist/*",
    "postbuild:dist": "find dist/ -type f -name '*.d.ts' -print0 | xargs -0 sed -i '' -e 's/\\/\\/\\/ <reference types=\"core-js\" \\/>//g'"
  },
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gizm0bill/angular2-rest-client.git"
  },
  "keywords": [
    "angular2",
    "http",
    "rest",
    "client"
  ],
  "author": "Mihai Billy Bălăceanu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gizm0bill/angular2-rest-client/issues"
  },
  "homepage": "https://github.com/gizm0bill/angular2-rest-client",
  "dependencies": {
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "core-js": "^2.5.7",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/platform-server": "^6.1.7",
    "@types/core-js": "^2.5.0",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.10.1",
    "@types/source-map": "^0.5.7",
    "@types/webpack": "^4.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "codelyzer": "^4.4.4",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "~3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "phantomjs": "^2.1.7",
    "rollup": "^0.66.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.1.1",
    "tslint": "^5.11.0",
    "typescript": "2.9",
    "webpack": "^4.19.1"
  }
}
